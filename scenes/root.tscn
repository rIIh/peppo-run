[sub_resource type="GDScript" id="GDScript_ulfc3"]
script/source = "@tool
extends ColorRect

var _safe_area: SafeArea

@onready
var _height = 0 :
	set(value):
		if size.y != value:
			size = Vector2(size.x, value)

		_height = value


func _enter_tree():
	_safe_area = _get_safe_area()
	_height = _safe_area.view_padding.bottom if _safe_area else 0


func _process(delta):
	if Engine.is_editor_hint():
		if not _safe_area:
			_safe_area = _get_safe_area()

		if _safe_area:
			_height = _safe_area.view_padding.bottom


func _exit_tree():
	_safe_area = null
	_height = 0


func _get_safe_area() -> SafeArea:
	var parent = get_parent()
	while parent and not parent is SafeArea:
		parent = parent.get_parent()

	return parent
"

